<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.juliagorelikova.musicalapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RussiaActivity"
            android:label="@string/Russia_l"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".FranceActivity"
            android:label="@string/French_l"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".GermanyActivity"
            android:label="@string/Germany_l"
            android:parentActivityName=".GermanyActivity"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>